	    <name>clicked</name>
	    <handler>remove_clicked</handler>
	    <last_modification_time>Sun, 23 Jun 2002 13:28:43 GMT</last_modification_time>
	  </signal>
	  <label>Remove</label>
	  <stock_pixmap>GNOME_STOCK_PIXMAP_TRASH</stock_pixmap>
	</widget>
      </widget>
    </widget>

    <widget>
      <class>GtkScrolledWindow</class>
      <child_name>GnomeDock:contents</child_name>
